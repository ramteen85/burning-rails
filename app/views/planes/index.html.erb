<p id="notice"><%= notice %></p>

<h1>Planes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>A1</th>
      <th>A2</th>
      <th>A3</th>
      <th>A4</th>
      <th>A5</th>
      <th>B1</th>
      <th>B2</th>
      <th>B3</th>
      <th>B4</th>
      <th>B5</th>
      <th>C1</th>
      <th>C2</th>
      <th>C3</th>
      <th>C4</th>
      <th>C5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @planes.each do |plane| %>
      <tr>
        <td><%= plane.name %></td>
        <td><%= plane.A1 %></td>
        <td><%= plane.A2 %></td>
        <td><%= plane.A3 %></td>
        <td><%= plane.A4 %></td>
        <td><%= plane.A5 %></td>
        <td><%= plane.B1 %></td>
        <td><%= plane.B2 %></td>
        <td><%= plane.B3 %></td>
        <td><%= plane.B4 %></td>
        <td><%= plane.B5 %></td>
        <td><%= plane.C1 %></td>
        <td><%= plane.C2 %></td>
        <td><%= plane.C3 %></td>
        <td><%= plane.C4 %></td>
        <td><%= plane.C5 %></td>
        <td><%= link_to 'Show', plane %></td>
        <td><%= link_to 'Edit', edit_plane_path(plane) %></td>
        <td><%= link_to 'Destroy', plane, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plane', new_plane_path %>
